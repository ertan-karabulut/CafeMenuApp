@{
    ViewData["Title"] = "Property Delete";
}
@model DetailPropertyDto

<form asp-action="DeleteProperty" method="post">
    <input type="hidden" asp-for="Id" />

    <div class="form-group">
        <a>Özellik Adı: @Model.Key</a>
    </div>

    <div class="form-group">
        <a>Değeri: @Model.Value</a>
    </div>

    <button type="submit" class="btn btn-primary">Sil</button>
    <a asp-action="List" class="btn btn-secondary">İptal</a>
 </form>
